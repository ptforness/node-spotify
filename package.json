{
  "name": "node-spotify",
  "version": "0.0.1",
  "description": "A spotify API wrapper for nodeJS",
  "main": "index.js",
  "type": "module",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "format": "prettier --write **/*.{ts,js}",
    "lint": "eslint **/*.{ts,js} --ext .js,.ts --cache --fix",
    "start": "node lib/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix",
    "*.{js,css,md}": "prettier --write"
  },
  "author": "Paul Forness",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.5.1",
    "@types/node": "^17.0.36",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-node": "^11.1.0",
    "husky": "^8.0.1",
    "jest": "^28.1.0",
    "lint-staged": "^12.4.2",
    "prettier": "^2.6.2",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "cross-fetch": "^3.1.5"
  }
}
